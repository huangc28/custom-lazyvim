{
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "trace.server": "verbose",
    }
  },

  "suggest.enablePreselect": false,
  "rest-client.enable": true,
  "rest-client.showHeaders" : true,
  "rest-client.proxy": "",
  "rest-client.proxyStrictSSL": false,
  "rest-client.excludeHostsForProxy": []
}
